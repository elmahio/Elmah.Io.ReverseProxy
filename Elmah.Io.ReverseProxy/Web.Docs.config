<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
    <customErrors mode="RemoteOnly" defaultRedirect="/internalservererror" xdt:Transform="InsertIfMissing" />
  </system.web>

  <system.webServer>
    <rewrite>
      <rules>
        <rule name="Proxy" stopProcessing="true" xdt:Transform="InsertIfMissing">
          <match url="(.*)" />
          <action type="Rewrite" url="http://elmahio.github.io/documentation/{R:1}" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>

</configuration>
